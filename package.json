{
  "name": "unitypackage2",
  "description": "Unity Package Creator and Extractor",
  "version": "1.0.0",
  "engines": {
    "node": ">=21.5.0"
  },
  "packageManager": "pnpm@8.14.1",
  "scripts": {
    "build": "tsc",
    "test": "tsc --noEmit && biome check .",
    "lint": "biome lint .",
    "format": "biome format . --write",
    "fix": "biome check . --apply",
    "postinstall": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "^1.5.1",
    "@types/node": "^20.10.8",
    "@types/tar-stream": "^3.1.3",
    "husky": "^8.0.3",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "tar-stream": "^3.1.7",
    "yaml": "^2.3.4"
  }
}
